{
  "extends": "tslint:latest",

  "rules": {
    "interface-name": false,
    "quotemark" : "single",

    "max-line-length": [true,
      140
    ],

     "no-reference": false,

     "object-literal-sort-keys": false,

    "no-namespace" : false,

    "member-ordering": [ true,
//    "public-before-private",
      "static-before-instance",
      "variables-before-functions"
    ]
  }
}
